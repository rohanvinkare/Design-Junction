/* Add your CSS styles here */
body {
    position: relative;
    margin: 0;
    overflow-x: hidden;
  }
  
  * {
    font-family: opensans;
  }
  
  /* ================== Start header ================== */
  /* style of section Top of header */
  .header {
    margin-bottom: 7.5rem;
  }
  .header__section--top {
    margin-top: 1rem;
  }
  .header__menu-container--top {
    display: flex;
    justify-content: space-between;
    align-items: center;
  }
  .header__menu-bar--left {
    width: 20rem;
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  .header__logo {
    width: 9.4rem;
  }
  .search-box {
    border: none;
    border-right: var(--secondary-color) solid 3px;
    border-radius: 2px;
    height: 29px;
  }
  .search-box:focus {
    outline: none;
  }
  .header__account-profile {
    align-items: center;
    gap: 10px;
    display: flex;
  }
  .header__account-profile--pict {
    width: 50px;
    height: 50px;
    border-radius: 100%;
    overflow: hidden;
  }
  .header__account-profile--pict img {
    width: 100%;
    height: 100%;
  }
  
  /* style of login botton */
  .header__login-btn {
    background-color: #fff;
    color: var(--primary-color);
    cursor: pointer;
    border: none;
    padding: 0.6rem 1.9rem;
    border-radius: 0.5rem;
    border: 0.15rem solid var(--primary-color);
    transition: 0.1s;
  }
  
  .header__login-btn:hover {
    background-color: var(--primary-color);
    color: #fff;
  }
  
  /* style of hambegurger menu */
  .header__burger-menu {
    width: 2rem;
  }
  .header__burger-menu:hover {
    color: var(--primary-color);
    cursor: pointer;
  }
  
  /* style of menu bar(after opening)*/
  .header__menu-bar--right ul {
    position: fixed;
    width: 18rem;
    height: 100%;
    z-index: 3;
    background: #fff;
    padding: 0;
    left: -22rem;
    top: 0;
    overflow: hidden;
    margin: 0;
    transition: 0.5s;
  }
  
  .header__menu-bar--right ul li {
    list-style-type: none;
    padding: 32px;
    background-color: #fff;
  }
  .header__menu-bar--right ul li:hover {
    background: #cbc6c6;
    border-right: 4px solid var(--primary-color);
  }
  
  .header__menu-bar--right ul li a {
    color: var(--primary-color);
    font-weight: bolder;
    font-family: alata;
    font-size: 23px;
    text-decoration: none;
  }
  
  /* black blur background style (when menu is open) */
  .overlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #0009;
    backdrop-filter: blur(5px);
    z-index: 2;
  }
  
  /* style of login-box (after open) */
  .login-box {
    position: fixed;
    z-index: 7;
    width: 100%;
    top: -18px;
    left: 0;
    height: 105%;
    margin: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    background-color: #0009;
    backdrop-filter: blur(5px);
  }
  
  .login-box__close-btn {
    font-size: 41px;
    width: 50px;
    height: 50px;
    position: absolute;
    text-align: center;
    right: 25px;
    top: 40px;
    cursor: pointer;
    color: #ff7f7f;
    border-radius: 100%;
  }
  .login-box__title {
    text-align: center;
    font-size: 26px;
    margin-top: 8px;
    margin-bottom: -12px;
    font-weight: bolder;
    color: var(--primary-color);
  }
  
  .login-box--form {
    position: relative;
    background-color: #fff;
    width: 22rem;
    border-radius: 13px;
    box-shadow: 0px 0px 20px 7px #000000a3;
    height: 27rem;
    display: flex;
    flex-direction: column;
    gap: 52px;
    padding: 1rem 2rem;
  }
  .login-box input {
    text-justify: center;
    height: 32px;
    font-size: 19px;
    padding: 5px 10px;
    border: 1px solid #6e6e6e;
    border-radius: 8px;
    background-color: #e6e6e6;
    outline: none;
    box-shadow: inset #4e4e4e 0px 0px 9px 0px;
    transition: 0.4s all;
    font-family: alata;
  }
  .login-box input[type="submit"] {
    width: 9rem;
    height: 2.6rem;
    background-color: var(--primary-color);
    color: #fff;
    letter-spacing: 3px;
    text-transform: uppercase;
    font-size: 16px;
    border-radius: 20px;
    border: none;
    cursor: pointer;
    align-self: center;
  }
  .login-box input:focus {
    transform: scale(1.03);
    background-color: white;
    border: none;
    box-shadow: #282828 4px 3px 9px 0px;
  }
  
  .login-box__pass--eye,
  .login-box__pass--eye-off {
    position: absolute;
    top: 12.5rem;
    right: 2.6rem;
    width: 24px;
    cursor: pointer;
  }
  
  /* section botton of haeder */
  .header__menu-container--bottom {
    display: flex;
    flex-direction: row-reverse;
    justify-content: space-between;
  }
  .header__section--botten--right {
    position: relative;
    width: 45%;
  }
  .header__img {
    width: 100%;
    border-radius: 100%;
  }
  .header__svg-background {
    width: 119%;
    fill: var(--primary-color);
    display: inline-block;
    position: absolute;
    right: 0;
    z-index: -1;
    object-fit: cover;
    object-position: center;
  }
  
  .header__title {
    font-family: ubuntu;
    font-size: 95px;
    margin-bottom: 0;
    font-weight: 100;
  }
  .header__text {
    max-width: 31rem;
    line-height: 2rem;
    font-size: 1.2rem;
  }
  
  .header__view-btn {
    letter-spacing: 3px;
    background-color: var(--primary-color);
    color: #fff;
    cursor: pointer;
    border: none;
    padding: 0.8rem 2.5rem;
    font-weight: 900;
    text-transform: uppercase;
    border-radius: 1.5rem;
    margin-top: 60px;
    border: 0.15rem solid var(--primary-color);
    transition: 0.1s;
  }
  .hidden {
    display: none;
  }

